{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "tsc: build - tsconfig.json"
		},
		{
			"label": "run",
			"type": "shell",
			"command": "tsc.cmd | npm run start",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}